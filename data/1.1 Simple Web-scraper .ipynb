   "source": [
    "# Web scraping example using BeautifulSoup\n",
    "import urllib\n",
    "from bs4 import BeautifulSoup\n",
    "with urllib.request.urlopen(\"https://www.google.com\") as url:\n",
    "    r = url.read()\n",
    "soup = BeautifulSoup(r, \"html.parser\")\n",
    "title = soup.find(\"title\")\n",
    "print(title)\n"
   ]
